      "numVaults()": {
        "details": "Returns the total number of vaults managed by the Controller."
      },
      "rewardToken()": {
        "details": "Returns the token served as additional rewards(i.e ACoconut) to vault users."
      },
      "treasury()": {
        "details": "Returns the treasury address for vault fees."
      },
      "vaults(uint256)": {
        "details": "Returns the vault address given the vault ID."
      }
